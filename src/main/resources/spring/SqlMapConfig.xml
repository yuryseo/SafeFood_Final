<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- MyBatis Query를 위한 Xml에서 resultType, parametertype 으로  
		     사용할  VO 클래스에 대한 별칭 설정 
		  Java AIP에 대한 별칭 설정 정보 : org.apache.ibits.type.TypeAliasRegistry 
		  -->
		<typeAlias type="com.ssafy.model.dto.Food"   		alias="food"/>
		<typeAlias type="com.ssafy.model.dto.MyFood"   		alias="myfood"/>
		<typeAlias type="com.ssafy.model.dto.Member"   		alias="member"/>
		<typeAlias type="com.ssafy.model.dto.Qboard"   		alias="qboard"/>
		<typeAlias type="com.ssafy.model.dto.Aboard"   		alias="aboard"/>
		<typeAlias type="com.ssafy.model.dto.PageBean"   	alias="pagebean"/>
		<typeAlias type="com.ssafy.model.dto.Wishlist"   	alias="wishlist"/>
		<typeAlias type="com.ssafy.model.dto.SearchMyFoodInfo"   	alias="searchmyfoodinfo"/>
	</typeAliases>
	<mappers>
	 <mapper resource="spring/food_query.xml" />
	 <mapper resource="spring/myfood_query.xml" />
	 <mapper resource="spring/member_query.xml" />
	 <mapper resource="spring/qna_query.xml" />
	 <mapper resource="spring/wishlist_query.xml" />
	</mappers>
</configuration>


















